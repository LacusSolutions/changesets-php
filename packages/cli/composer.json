{
  "name": "lacus/changesets-cli",
  "version": "1.0.0",
  "type": "library",
  "description": "CLI interface for PHP changesets",
  "license": "MIT",
  "authors": [
    {
      "name": "Julio L. Muller",
      "email": "juliolmuller@outlook.com",
      "homepage": "https://juliolmuller.github.io"
    }
  ],
  "keywords": [],
  "bin": [
    "bin/changesets"
  ],
  "scripts": {},
  "config": {
    "sort-packages": true
  },
  "require": {
    "php": "^8.1",
    "lacus/changesets-apply-release-plan": "*",
    "lacus/changesets-assemble-release-plan": "*",
    "lacus/changesets-config": "*",
    "lacus/changesets-get-dependents-graph": "*",
    "lacus/changesets-git": "*",
    "lacus/changesets-parse": "*",
    "lacus/changesets-read": "*",
    "lacus/changesets-types": "*",
    "lacus/changesets-write": "*",
    "symfony/console": "^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5",
    "spatie/phpunit-watcher": "~1.24"
  },
  "autoload": {
    "psr-4": {
      "Lacus\\Changesets\\Cli\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Lacus\\Changesets\\Cli\\Tests\\": "tests/"
    }
  }
}
